<h1>Editing version</h1>

<% form_for(@version) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :object_id %><br />
    <%= f.text_field :object_id %>
  </p>
  <p>
    <%= f.label :object_type %><br />
    <%= f.text_field :object_type %>
  </p>
  <p>
    <%= f.submit 'Update' %>
  </p>
<% end %>

<%= link_to 'Show', @version %> |
<%= link_to 'Back', versions_path %>